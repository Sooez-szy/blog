<script type="text/javascript">
		$(document).ready(function() {
			var pageCount = $("#countInput").val();
			$("#page").tzPage(pageCount,{
				num_edge_entries : 1, //边缘页数
				num_display_entries :4, //主体页数
				num_edge_entries:5,
				current_page:0,
				showGo:true,
				showSelect:true,
				items_per_page : 3, //每页显示X项
				prev_text : "前一页",
				next_text : "后一页",
				callback : function(pageNo,psize){//回调函数
					loadData(pageNo*psize,psize);
				}
			});
			function loadData(pageNo,psize){
				var opid = $("body").data("opid");
				console.log(pageNo)
				$.ajax({
					type:'post',
					url:"/article",
					data:{"pageNo":pageNo,"pageSize":psize},
					success:function(data){
						$("#articleContent").html(data);
					}
				})
			}
			loadData(0,3)
		$().UItoTop({ easingType: 'easeOutQuart' });
});
</script>
